      <core>
	import VR
	k = dev.getKey()
	s = dev.getState()
	m = dev.getMessage()
	print k,s,m
	
	if s == 1: return
	
	site = ''
	if k == 101:
		site = 'http://www.imi.kit.edu'
	if k == 102:
		site = 'http://www.kit.de'
	if k == 103:
		site = 'http://www.google.de'
	
	if hasattr(VR, 'web'):
		VR.web.open(site)
